{
  "name": "apa-quote-formatter",
  "version": "1.0.0",
  "description": "Formatador automático de aspas para documentos acadêmicos segundo normas APA 7ª edição",
  "main": "apa-quote-formatter.js",
  "scripts": {
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "dev-server": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "validate": "office-addin-manifest validate manifest.xml",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "dev-certs": "office-addin-dev-certs install"
  },
  "keywords": [
    "office-add-in",
    "word",
    "apa",
    "academic",
    "formatting",
    "quotes",
    "typescript"
  ],
  "author": "Academic Tools Team",
  "license": "MIT",
  "dependencies": {
    "@microsoft/office-js": "^1.1.83",
    "@microsoft/office-js-helpers": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/office-js": "^1.0.307",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^8.35.0",
    "html-webpack-plugin": "^5.6.6",
    "jest": "^29.5.0",
    "office-addin-debugging": "^5.0.5",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-manifest": "^1.12.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.0.0",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}